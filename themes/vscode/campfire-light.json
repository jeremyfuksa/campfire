  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9299a5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#a8654f"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#ef991f"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#a8654f"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#2b303b"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5e6371"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#4c627d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#607a97"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#739038"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#8098b0"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#4c627d"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#739038"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#739038"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#8899b3"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#5e6371"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#9299a5"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#4c627d"
      }
    },
    {
      "scope": ["meta.brace", "meta.bracket", "meta.parens"],
      "settings": {
        "foreground": "#5e6371"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#dc3a38",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ef991f"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#4c627d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#2b303b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#5e6371",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#9299a5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#739038"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#4c627d"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#739038"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#dc3a38"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ef991f"
      }
    }
  ]
}
