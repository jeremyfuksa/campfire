  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#747b8a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#c9998a"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#f9c574"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#dbbdb3"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#f7f8f9"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9299a5"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#acbbcc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8098b0"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#8fb14b"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#acbbcc"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8098b0"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8fb14b"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#8fb14b"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#b8c5d9"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#9299a5"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#747b8a"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#acbbcc"
      }
    },
    {
      "scope": ["meta.brace", "meta.bracket", "meta.parens"],
      "settings": {
        "foreground": "#9299a5"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#e75351",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#f9c574"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#acbbcc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#f7f8f9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#9299a5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#747b8a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#8fb14b"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#8098b0"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#8fb14b"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#e75351"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#f9c574"
      }
    }
  ]
}
